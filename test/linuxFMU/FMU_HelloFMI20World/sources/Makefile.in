# Makefile generated by OpenModelica
CC=@CC@
AR=@AR@
CFLAGS=@CFLAGS@
LD=$(CC) -shared
LDFLAGS=@LDFLAGS@ @LIBS@
DLLEXT=@DLLEXT@
NEED_RUNTIME=@NEED_RUNTIME@
NEED_DGESV=@NEED_DGESV@
FMIPLATFORM=@FMIPLATFORM@
# Note: Simulation of the fmu with dymola does not work with -finline-small-functions (enabled by most optimization levels)
CPPFLAGS=@CPPFLAGS@
OMC_NUM_LINEAR_SYSTEMS=0
OMC_NUM_NONLINEAR_SYSTEMS=0
OMC_NUM_MIXED_SYSTEMS=0
OMC_NDELAY_EXPRESSIONS=0
OMC_NVAR_STRING=0

override CPPFLAGS += -Iinclude/ -Iinclude/fmi2 -I.    -DOMC_MODEL_PREFIX=HelloFMI20World -DOMC_NUM_MIXED_SYSTEMS=0 -DOMC_NUM_LINEAR_SYSTEMS=0 -DOMC_NUM_NONLINEAR_SYSTEMS=0 -DOMC_NDELAY_EXPRESSIONS=0 -DOMC_NVAR_STRING=0

MAINFILE=HelloFMI20World_FMU.c
MAINOBJ=HelloFMI20World_FMU.o
CFILES=HelloFMI20World.c HelloFMI20World_functions.c HelloFMI20World_records.c \
HelloFMI20World_01exo.c HelloFMI20World_02nls.c HelloFMI20World_03lsy.c HelloFMI20World_04set.c HelloFMI20World_05evt.c HelloFMI20World_06inz.c HelloFMI20World_07dly.c \
HelloFMI20World_08bnd.c HelloFMI20World_09alg.c HelloFMI20World_10asr.c HelloFMI20World_11mix.c HelloFMI20World_12jac.c HelloFMI20World_13opt.c HelloFMI20World_14lnz.c \
HelloFMI20World_15syn.c HelloFMI20World_16dae.c HelloFMI20World_17inl.c HelloFMI20World_init_fmu.c
OFILES=$(CFILES:.c=.o)
GENERATEDFILES=$(MAINFILE) HelloFMI20World_FMU.makefile HelloFMI20World_literals.h HelloFMI20World_model.h HelloFMI20World_includes.h HelloFMI20World_functions.h  HelloFMI20World_11mix.h HelloFMI20World_12jac.h HelloFMI20World_13opt.h HelloFMI20World_init_fmu.c HelloFMI20World_info.c $(CFILES) HelloFMI20World_FMU.libs

# FIXME: before you push into master...
RUNTIMEDIR=include
OMC_MINIMAL_RUNTIME=1
OMC_FMI_RUNTIME=1
include $(RUNTIMEDIR)/Makefile.objs
ifneq ($(NEED_RUNTIME),)
RUNTIMEFILES=$(FMI_ME_OBJS:%=$(RUNTIMEDIR)/%.o)
endif

PHONY: HelloFMI20World_FMU
HelloFMI20World_FMU: nozip
	cd .. && rm -f ../HelloFMI20World.fmu && zip -r ../HelloFMI20World.fmu *
nozip: $(MAINOBJ) HelloFMI20World_functions.h HelloFMI20World_literals.h $(OFILES) $(RUNTIMEFILES)
	mkdir -p ../binaries/$(FMIPLATFORM)
ifeq (@LIBTYPE_DYNAMIC@,1)
	$(LD) -o HelloFMI20World$(DLLEXT) $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)    $(LDFLAGS)
	cp HelloFMI20World$(DLLEXT) HelloFMI20World_FMU.libs ../binaries/$(FMIPLATFORM)/
endif
	head -n20 Makefile > ../resources/$(FMIPLATFORM).summary
ifeq (@LIBTYPE_STATIC@,1)
	rm -f HelloFMI20World.a
	$(AR) -rsu HelloFMI20World.a $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)
	cp HelloFMI20World.a HelloFMI20World_FMU.libs ../binaries/$(FMIPLATFORM)/
endif
	$(MAKE) distclean
distclean: clean
	rm -f Makefile config.status config.log
clean:
	rm -f HelloFMI20World.def HelloFMI20World.o HelloFMI20World.a HelloFMI20World$(DLLEXT) $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)