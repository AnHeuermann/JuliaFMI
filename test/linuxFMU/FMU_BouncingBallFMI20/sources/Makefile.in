# Makefile generated by OpenModelica
CC=@CC@
AR=@AR@
CFLAGS=@CFLAGS@
LD=$(CC) -shared
LDFLAGS=@LDFLAGS@ @LIBS@
DLLEXT=@DLLEXT@
NEED_RUNTIME=@NEED_RUNTIME@
NEED_DGESV=@NEED_DGESV@
FMIPLATFORM=@FMIPLATFORM@
# Note: Simulation of the fmu with dymola does not work with -finline-small-functions (enabled by most optimization levels)
CPPFLAGS=@CPPFLAGS@
OMC_NUM_LINEAR_SYSTEMS=0
OMC_NUM_NONLINEAR_SYSTEMS=0
OMC_NUM_MIXED_SYSTEMS=0
OMC_NDELAY_EXPRESSIONS=0
OMC_NVAR_STRING=0

override CPPFLAGS += -Iinclude/ -Iinclude/fmi2 -I.    -DOMC_MODEL_PREFIX=BouncingBallFMI20 -DOMC_NUM_MIXED_SYSTEMS=0 -DOMC_NUM_LINEAR_SYSTEMS=0 -DOMC_NUM_NONLINEAR_SYSTEMS=0 -DOMC_NDELAY_EXPRESSIONS=0 -DOMC_NVAR_STRING=0

MAINFILE=BouncingBallFMI20_FMU.c
MAINOBJ=BouncingBallFMI20_FMU.o
CFILES=BouncingBallFMI20.c BouncingBallFMI20_functions.c BouncingBallFMI20_records.c \
BouncingBallFMI20_01exo.c BouncingBallFMI20_02nls.c BouncingBallFMI20_03lsy.c BouncingBallFMI20_04set.c BouncingBallFMI20_05evt.c BouncingBallFMI20_06inz.c BouncingBallFMI20_07dly.c \
BouncingBallFMI20_08bnd.c BouncingBallFMI20_09alg.c BouncingBallFMI20_10asr.c BouncingBallFMI20_11mix.c BouncingBallFMI20_12jac.c BouncingBallFMI20_13opt.c BouncingBallFMI20_14lnz.c \
BouncingBallFMI20_15syn.c BouncingBallFMI20_16dae.c BouncingBallFMI20_17inl.c BouncingBallFMI20_init_fmu.c
OFILES=$(CFILES:.c=.o)
GENERATEDFILES=$(MAINFILE) BouncingBallFMI20_FMU.makefile BouncingBallFMI20_literals.h BouncingBallFMI20_model.h BouncingBallFMI20_includes.h BouncingBallFMI20_functions.h  BouncingBallFMI20_11mix.h BouncingBallFMI20_12jac.h BouncingBallFMI20_13opt.h BouncingBallFMI20_init_fmu.c BouncingBallFMI20_info.c $(CFILES) BouncingBallFMI20_FMU.libs

# FIXME: before you push into master...
RUNTIMEDIR=include
OMC_MINIMAL_RUNTIME=1
OMC_FMI_RUNTIME=1
include $(RUNTIMEDIR)/Makefile.objs
ifneq ($(NEED_RUNTIME),)
RUNTIMEFILES=$(FMI_ME_OBJS:%=$(RUNTIMEDIR)/%.o)
endif

PHONY: BouncingBallFMI20_FMU
BouncingBallFMI20_FMU: nozip
	cd .. && rm -f ../BouncingBallFMI20.fmu && zip -r ../BouncingBallFMI20.fmu *
nozip: $(MAINOBJ) BouncingBallFMI20_functions.h BouncingBallFMI20_literals.h $(OFILES) $(RUNTIMEFILES)
	mkdir -p ../binaries/$(FMIPLATFORM)
ifeq (@LIBTYPE_DYNAMIC@,1)
	$(LD) -o BouncingBallFMI20$(DLLEXT) $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)    $(LDFLAGS)
	cp BouncingBallFMI20$(DLLEXT) BouncingBallFMI20_FMU.libs ../binaries/$(FMIPLATFORM)/
endif
	head -n20 Makefile > ../resources/$(FMIPLATFORM).summary
ifeq (@LIBTYPE_STATIC@,1)
	rm -f BouncingBallFMI20.a
	$(AR) -rsu BouncingBallFMI20.a $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)
	cp BouncingBallFMI20.a BouncingBallFMI20_FMU.libs ../binaries/$(FMIPLATFORM)/
endif
	$(MAKE) distclean
distclean: clean
	rm -f Makefile config.status config.log
clean:
	rm -f BouncingBallFMI20.def BouncingBallFMI20.o BouncingBallFMI20.a BouncingBallFMI20$(DLLEXT) $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)